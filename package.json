    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.4",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-pwa-manifest": "^4.3.0"
  }
